{
	"name": "new-website",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"biome:check": "biome ci src postcss.config.cjs svelte.config.js tailwind.config.ts vite.config.ts",
		"biome:format": "biome format --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.2",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.48.3",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/postcss": "^4.1.16",
		"@tailwindcss/vite": "^4.1.16",
		"postcss": "^8.5.6",
		"svelte": "^5.43.2",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^4.1.16",
		"typescript": "^5.9.2",
		"vite": "^7.1.7"
	},
	"dependencies": {
		"daisyui": "^5.3.10",
		"rollup": "^4.52.5"
	}
}
